    $('#splisttab-title span').hover(function(){
        $(this).addClass("selected").siblings().removeClass();
        $("#splisttab-content > div").hide().eq($('#splisttab-title span').index(this)).show();
    });
    
    //视频壁纸mask
    $('.splist_ul li').hover(function() { 
        $(this).find('.spbzmask_span').hide(); 
        $(this).find('.spimg_div').css('border-color','#ffd993');
    }, function() { 
        $(this).find('.spbzmask_span').show();
        $(this).find('.spimg_div').css('border-color','#3f3d6a'); 
    });*/
    
    /*右侧边收缩展开*/ 
    $('#side_right_xiao').click(function(){
        $('.side-nav-right-x').animate({
            right: '-61'
        },400);
        $('.side-nav-right').animate({
            right: '0',
            opacity: 'show'
        }, 800 );
    });
    $('.rightside_close').click(function(){
        $('.side-nav-right-x').animate({
            right: '0',
            opacity: 'show'
        },400);
        $('.side-nav-right').animate({
            right: '-201',
            opacity: 'show'
        }, 800 );
    });
    
    $('.nei-ios-a').click(function(){
        //console.log('111');
        $('.r-xzmar01').show();
        $('.r-xzmar02').hide();
    });
    $('.nei-android-a').click(function(){
        $('.r-xzmar02').show();
        $('.r-xzmar01').hide();
    });
    
    /*弹出层手机注册和49账号注册切换*/    
    $('.you_49_a').click(function(){
        if($('.reg_mobile').css('display') == 'block')
        {
            $('.you_49_a').addClass('mobile_a');
            $('.reg_mobile').hide();
            $('.reg_49you').show();
        }else{
            $('.you_49_a').removeClass('mobile_a');
            $('.reg_mobile').show();
            $('.reg_49you').hide();
        }
    });
    
    //登录
    $('.loginbtn_a').click(function(){
        $('.reg_wrap01').hide();
        $('.reg_wrap02').show();
    });
    
    //退出
    $('.index_tc').click(function(){
        $('.reg_wrap02').hide();
        $('.reg_wrap01').show();
    });
    
});
