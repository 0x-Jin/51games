<script type="text/javascript">
$(function(){
    
	//切换效果
	$('#tab-title span').click(function(){
		$(this).addClass("selected").siblings().removeClass('selected');
		var t=$('#tab-title span').index(this);
		$("#tab-content > div").hide().eq(t).show();
		
	});
	
	//职业展示
	$('#zy_01').click(function(){
      $("#zy_02").show();
	  $("#zy_04").hide();
    });
	
	$('#zy_03').click(function(){
      $("#zy_02").hide();
	  $("#zy_04").show();
    });
	
	
});
</script>
